{
  "rewrites": [
    { "source": "/about", "destination": "/about-our-company.html" },
    { "source": "/resize/:width/:height", "destination": "/api/sharp" },
    {
      "source": "/og",
      "destination": "https://empty-8zx6gtj2l-hackerone-vercel-testing-environment.vercel.app/"
